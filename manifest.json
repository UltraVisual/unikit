{
	"name": "Unicorn Toolkit",
	"version": "1.0",
	"minimum_chrome_version": "10.0",
	"devtools_page": "devtools.html",
	"manifest_version": 2,
	"browser_action": {
		"default_title": "Unicorn Toolkit",
		"default_popup": "popups/popup.html"
	},
	"permissions": [
	"webRequest",
	"webRequestBlocking",
		"*://*/*"
	],
	"background": {
		"scripts": ["background.js"]
	}
}
